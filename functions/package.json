{
  "author": "Henry Faulkner",
  "name": "artists-saved-songs-playlists",
  "description": "This is app is meant to create a playlist that imitates Spotify's deprecated \"listen to saved songs by specific artist in the Your Library\" functionality.",
  "version": "0.0.1",
  "main": "build/server.js",
  "scripts": {
    "start": "npx functions-framework --source=build/server.js --target=expressAPI",
    "local": "npm run compile-sass; npx nodemon functions/server.ts",
    "build": "tsc --outDir build",
    "watch-sass": "node-sass --watch ./public/styles/scss -o ./public/styles/css",
    "compile-sass": "node-sass ../public/styles/scss -o ../public/styles/css"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "dotenv": "^16.0.1",
    "express": "~4.16.0",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^3.21.2",
    "lodash.isequal": "^4.5.0",
    "node-sass": "^7.0.1",
    "querystring": "~0.2.0",
    "request": "~2.83.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "nodemon": "^2.0.16"
  }
}
